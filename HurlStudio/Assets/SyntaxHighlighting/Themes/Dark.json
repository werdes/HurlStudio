{
  "$schema": "vscode://schemas/color-theme",
  "name": "Dark",
  "tokenColors": [
    {
      "name": "Dynamic Keywords",
      "scope": [ "keyword.method" ],
      "settings": {
        "foreground": "#4EC9B0",
        "fontStyle": "Bold"
      }
    },
    {
      "name": "Static Keywords",
      "scope": [ "keyword.http" ],
      "settings": {
        "foreground": "#C586C0",
        "fontStyle": "Bold"
      }
    },
    {
      "name": "Section Keywords",
      "scope": [ "keyword.section" ],
      "settings": {
        "foreground": "#6991b2",
        "fontStyle": "Bold"
      }
    },
    {
      "name": "Response HTTP Statuscode",
      "scope": [ "response.statuscode" ],
      "settings": {
        "foreground": "#b5cea8",
        "fontStyle": "Bold"
      }
    },
    {
      "name": "Comments",
      "scope": [ "notation.comment" ],
      "settings": {
        "foreground": "#6adb6d",
        "fontStyle": "Bold"
      }
    },
    {
      "name": "Variables",
      "scope": [ "notation.variable" ],
      "settings": {
        "foreground": "#e682da",
        "fontStyle": "Bold"
      }
    },
    {
      "name": "Key Value: Key",
      "scope": [ "notation.keyvalue.key" ],
      "settings": {
        "foreground": "#96d0ff",
        "fontStyle": "Bold"
      }
    },
    {
      "name": "Parentheses and Brackets",
      "scope": [
        "notation.parentheses.open",
        "notation.parentheses.close",
        "notation.brackets.open",
        "notation.brackets.close",
        "notation.quotes.single"
      ],
      "settings": {
        "foreground": "#e8618b",
        "fontStyle": "Bold"
      }
    },
    {
      "name": "Quotes",
      "scope": [
        "notation.quotes.double",
        "notation.quotes.single"
      ],
      "settings": {
        "foreground": "#d69d85",
        "fontStyle": "Bold"
      }
    },
    {
      "name": "URLs",
      "scope": [
        "notation.url"
      ]
    },
    {
      "name": "Assertions & Filters",
      "scope": [
        "keyword.assertion.query",
        "keyword.assertion.predicate",
        "keyword.filter"
      ],
      "settings": {
        "foreground": "#3665d5",
        "fontStyle": "Bold"
      }
    }
  ]
}